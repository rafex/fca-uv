	                <name>mvnrepository</name>
	                <url>http://www.mvnrepository.com</url>
	            </repository>
	        </repositories>
    	</profile>
       

    </profiles>

    <activeProfiles>
        <activeProfile>mvnrepository</activeProfile>
        
    </activeProfiles>

