    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <!-- <localRepository>{Cuando se desea usar un repositorio en una ruta diferente en local}</localRepository> -->

    
    
    

<!-- Si se requiere usar proxy agregar aquÃ­ los datos -->
    <proxies>
        <proxy>
            <active>false</active>
            <protocol>http</protocol>
            <host>150.100.210.40</host>
            <port>8080</port>
            <username></username>
            <password></password>
